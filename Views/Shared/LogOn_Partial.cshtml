@if(Request.IsAuthenticated) {
    <text><strong> Logged in as: @Html.ActionLink(@User.Identity.Name, "EditUser/" + @User.Identity.Name, "Account", null, new { @style="color:#FFFFFF;"})</strong> 
    [ @Html.ActionLink("Log Off", "LogOff", "Account", null, new { @style="color:#FFFFFF;" }) ] </text>
}
else {
    @:[ @Html.ActionLink("Log On", "LogOn", "Account", null, new { @style="color:#FFFFFF;" }) ]
}
